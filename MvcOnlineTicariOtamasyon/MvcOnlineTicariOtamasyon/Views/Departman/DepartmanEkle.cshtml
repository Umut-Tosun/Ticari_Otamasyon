@model MvcOnlineTicariOtamasyon.Models.Sınıflar.Departman
@{
    ViewBag.Title = "Departmankle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Yeni Departman Ekleme Sayfası</h2>
<br />
<form class="form-group" method="post">
    @Html.Label("Departman AD")
    @Html.TextBoxFor(x => x.DepartmanAd, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-primary">Yeni Departman Ekle</button>
</form>

